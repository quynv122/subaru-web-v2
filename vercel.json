{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "page/**/*", "use": "@vercel/static" },
    { "src": "assets/**/*", "use": "@vercel/static" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/$", "status": 302, "headers": { "Location": "/page/index.html" } },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
